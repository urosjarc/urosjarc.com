{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "test": "ng test --watch",
    "start": "ng serve",
    "start-en": "ng serve --port=4201 --configuration=development-en",
    "build": "ng build --localize && npx http-server -c-1 -p 8080 dist/app",
    "watch": "ng build --localize --watch --configuration development",
    "localize": "ng extract-i18n --format=json --output-path src/locale",
    "localize-fix": "meld src/locale/messages.json src/locale/messages.en.json",
    "openapi-fix": "ts-node ./scripts/fix-ng-openapi-gen.ts",
    "openapi": "ng-openapi-gen"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/cdk": "^16.1.6",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/material": "^16.1.6",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@swimlane/ngx-charts": "^20.4.1",
    "bootstrap": "^5.3.1",
    "dexie": "^3.2.4",
    "iso8601-duration": "^2.1.1",
    "moment": "^2.29.4",
    "ngx-logger": "^5.0.12",
    "rxjs": "~7.8.0",
    "simple-statistics": "^7.8.3",
    "tslib": "^2.3.0",
    "typesafe-routes": "^10.0.4",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.6",
    "@angular/cli": "~16.1.6",
    "@angular/compiler-cli": "^16.1.0",
    "@angular/localize": "^16.1.7",
    "@types/d3": "^7.4.0",
    "@types/jasmine": "~4.3.0",
    "ajv": "^8.12.0",
    "cypress": "^13.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-mocks": "^14.11.0",
    "ng-openapi-gen": "^0.25.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.3"
  }
}
