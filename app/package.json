{
	"name": "app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"generate-venv": "python3 -m venv venv && ./venv/bin/pip install -r requirements.txt",
		"generate-api": "rm -fr src/api && openapi --postfix Api --exportSchemas true --input http://localhost:8000/schema --output ./src/api --client axios",
		"scripts-api": "./venv/bin/python scripts/api.py",
		"scripts-sitemap": "./venv/bin/python scripts/sitemap.py",
		"scripts-upload": "./venv/bin/python scripts/upload.py",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@material/typography": "^14.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.5.0",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0"
	},
	"type": "module",
	"dependencies": {
		"@smui/button": "^7.0.0-beta.8",
		"@smui/common": "^7.0.0-beta.8",
		"@smui/fab": "^7.0.0-beta.8",
		"@smui/icon-button": "^7.0.0-beta.8",
		"@smui/layout-grid": "^7.0.0-beta.8",
		"@smui/list": "^7.0.0-beta.8",
		"@smui/textfield": "^7.0.0-beta.8",
		"smui-theme": "^7.0.0-beta.8",
		"svelte-local-storage-store": "^0.5.0"
	}
}
